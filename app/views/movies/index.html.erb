<%= button_to "Return To Discover Page", "/users/#{@user.id}/discover", method: :get %>
<% @movies.each do |movie| %>
  <section id="movie-<%= movie[:id] %>">
    <%= "Movie Title:" %>
    <%= link_to "#{movie[:title]}", "/users/#{@user.id}/movies/#{movie[:id]}" %><br>
    <%= "Vote Average: #{movie[:vote_average]}" %><br><br>
  </section>
<% end %>